                                    window.xt_ac = user.selection_code;
                                }

                                if (element) {
                                    window.xt_multc += '&x8=' + (restreint ? '23' : '13');
                                }

                                xiti.init();
                            }
                        } else {
                            xiti.init();
                        }
                    });
                });
            }
        }
